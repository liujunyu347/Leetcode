class Solution:
    def twoSum(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: List[int]
        """
        d = dict()
        for i in range(len(nums)):
            diff = target - nums[i]
            if diff in d:
                return [d[diff],i]
            else:
                d[nums[i]] = i
        
